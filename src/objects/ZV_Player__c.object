    <validationRules>
        <fullName>Human_Player_has_User</fullName>
        <active>true</active>
        <description>If player is human, User must have a lookup value.</description>
        <errorConditionFormula>AND(
  $RecordType.Name = &quot;Human&quot;,
   ISBLANK( User__c)
)</errorConditionFormula>
        <errorDisplayField>User__c</errorDisplayField>
        <errorMessage>If player is human, User must have a lookup value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Human_should_Not_have_a_Strategy</fullName>
        <active>true</active>
        <description>Human players should not have a strategy.</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Human&apos;
&amp;&amp; NOT(ISBLANK( StrategyClassName__c ))</errorConditionFormula>
        <errorDisplayField>StrategyClassName__c</errorDisplayField>
        <errorMessage>Human players should not have a strategy.</errorMessage>
    </validationRules>
    <validationRules>
